{{!< layout}}
    <div class="row">
        <div class="col-sm-12">
            <h1>{{title}}</h1>
        </div>
    </div>
    <div class="row">
        <div class="col-sm-12">
            <div class="wrap">
            <p class="form-title">
                Log In
            </p>
            <form class="login">
                <input name="username" type="text" placeholder="Username" />
                <input name="password" type="password" placeholder="Password" />
                <input type="submit" value="Login In" class="btn btn-success btn-sm" />
            </form>
        </div>
        </div>
    </div>


